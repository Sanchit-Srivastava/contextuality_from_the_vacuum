    "'''This cell imports the necessary libraries and modules for the analysis.'''\\n",
    "import sys, os\\n",
    "from pathlib import Path\\n",
    "\\n",
    "# Robustly ensure 'src' is on sys.path (works whether CWD is repo root or notebooks/)\\n",
    "for p in [Path.cwd(), *Path.cwd().parents]:\\n",
    "    cand = p / \\"src\\"\\n",
    "    if cand.exists():\\n",
    "        sys.path.insert(0, str(cand))\\n",
    "        break\\n",
    "\\n",
    "import numpy as np\\n",
    "import matplotlib.pyplot as plt\\n",
    "import warnings\\n",
    "warnings.filterwarnings(\\"ignore\\", category=UserWarning)\\n",
    "warnings.filterwarnings(\\"ignore\\", category=RuntimeWarning)\\n",
    "\\n",
    "# Importing modules\\n",
    "from qft.udw_qutrits import detector_state      \\n",
    "from optimization.lin_prog import contextual_fraction\\n",
    "from utils.state_checks import is_valid_state, validate_and_print\\n",
    "\\n",
    "\\n",
    "plt.rcParams.update({\\n",
    "    \\"text.usetex\\": True,\\n",
    "    \\"text.latex.preamble\\": r\\"\\\\usepackage{lmodern}\\",\\n",
    "    \\"font.family\\": \\"serif\\",\\n",
    "    \\"font.serif\\": [\\"Computer Modern Roman\\"],\\n",
    "})\\n",
    "\\n",
    "\\n",
    "def reduce_state(rho: np.ndarray) -> np.ndarray:\\n",
    "    \\"\\"\\"\\n",
    "    Partial trace over the second qutrit (B) of a 2-qutrit density matrix.\\n",
    "\\n",
    "    Parameters\\n",
    "    ----------\\n",
    "    rho : np.ndarray\\n",
    "        9x9 density matrix for a 2-qutrit system (AâŠ—B), dtype complex recommended.\\n",
    "\\n",
    "    Returns\\n",
    "    -------\\n",
    "    np.ndarray\\n",
    "        3x3 reduced density matrix for the first qutrit (A).\\n",
    "    \\"\\"\\"\\n",
    "    rho = np.asarray(rho)\\n",
    "    if rho.shape != (9, 9):\\n",
    "        raise ValueError(\\"rho must be a 9x9 matrix for a 2-qutrit system.\\")\\n",
    "\\n",
    "    # Reshape to indices [i_A, i_B, j_A, j_B]\\n",
    "    rho_reshaped = rho.reshape(3, 3, 3, 3)\\n",
    "\\n",
    "    # Trace over B: sum over i_B == j_B\\n",
    "    rho_A = np.trace(rho_reshaped, axis1=1, axis2=3)  # shape (3,3)\\n",
    "\\n",
    "    return rho_A\\n",
    "\\n"
   ]
